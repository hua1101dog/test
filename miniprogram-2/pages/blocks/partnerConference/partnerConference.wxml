<scroll-view scroll-y class="bgcontainer">
  <view class="companys">
    <block wx:for="{{partners}}" wx:key="unique">
      <view class="each" bindtap="toPartnerdetail" data-item="{{item}}">
        <view class="name">
          {{item.name}}
        </view>
        <view class="tag">
          {{item.tag}}
        </view>
      </view>
    </block>
  </view>
</scroll-view>